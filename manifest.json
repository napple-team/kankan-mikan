{
  "manifest_version": 2,
  "name": "かんかんみかん",
  "version": "2.0.0",
  "description": "右クリックしたリンクのURLをWebhookに送信します",
  "icons": {
    "48": "icons/icon48.png",
    "96": "icons/icon96.png"
  },
  "permissions": [
    "contextMenus",
    "storage",
    "notifications"
  ],
  "options_ui": {
    "page": "option/index.html"
  },
  "background": {
    "scripts": [
      "background/index.js"
    ]
  }
}
